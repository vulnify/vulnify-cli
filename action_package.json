{
  "name": "vulnify-action",
  "version": "1.0.0",
  "description": "GitHub Action for Vulnify Security Scanner",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build index.js -o dist --source-map --license licenses.txt",
    "package": "npm run build",
    "test": "jest",
    "lint": "eslint index.js",
    "all": "npm run lint && npm run build && npm test"
  },
  "repository": {
    "type": "git",
